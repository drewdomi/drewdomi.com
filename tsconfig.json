{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "noUnusedParameters": false,
    "noImplicitAny": false,
    "strict": false,
    "allowJs": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["node_modules", ".vercel", "dist", ".astro", "public"]
}

---
import { getEntry } from 'astro:content'
import ProjectPost from '../../../layouts/ProjectPost.astro'

const { slug } = Astro.params
const projects = await getEntry('projects', `${slug}`)

if (!projects) {
  return Astro.redirect('/404')
}

const { Content } = await projects.render()
---

<ProjectPost {...projects.data}>
  <Content />
</ProjectPost>
